<p id="notice"><%= notice %></p>

<h1>Invoice States</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoice_states.each do |invoice_state| %>
      <tr>
        <td><%= invoice_state.Name %></td>
        <td><%= link_to 'Show', invoice_state %></td>
        <td><%= link_to 'Edit', edit_invoice_state_path(invoice_state) %></td>
        <td><%= link_to 'Destroy', invoice_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice State', new_invoice_state_path %>
